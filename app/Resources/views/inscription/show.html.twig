{% extends 'base.html.twig' %}

{% block body %}
    <h1>Inscription</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ inscription.id }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ inscription.titre }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ inscription.prix }}</td>
            </tr>
            <tr>
                <th>Categorie</th>
                <td>{{ inscription.categorie }}</td>
            </tr>
            <tr>
                <th>Path</th>
                <td>{{ inscription.path }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('adminInscription_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('adminInscription_edit', { 'id': inscription.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
